{
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-config-prettier"
  ],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts"
    ]
  },
  "rules": {
    "no-console": false,
    "typedef": [
      true,
      "call-signature",
      "parameter",
      "member-variable-declaration",
      "property-declaration"
    ]
  }
}