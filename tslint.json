{
  "extends": [
    "tslint:recommended",
    "tslint-react"
  ],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts"
    ]
  },
  "rules": {
    "no-console": false,
    "typedef": [
      true,
      "call-signature",
      "parameter",
      "member-variable-declaration",
      "property-declaration"
    ],
    "interface-over-type-literal": false
  }
}